{
    "version": 2,
    "builds": [
      {
        "src": "backportfolio/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "backportfolio/wsgi.py"
      }
    ]
  }
  